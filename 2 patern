package session4;

public class ques1 {

	public static void main(String[] args) {
			int n = 5;
			int row = 1;
			int nst = n; // number of stars
			
			while(row <= n) {
				int cst = 1;
			 while(cst <= nst) {
				 System.out.print(" * ");
				 cst = cst + 1; // current stars
			 }
			 
			 System.out.println();
			 row+=1;
			 nst-=1;
			}
		}
	}
