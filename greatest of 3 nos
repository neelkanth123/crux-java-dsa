package session3;

public class greatestofthreenos {
	public static void main(String[] args) {
	int a = 30;
	int b = 50;
	int c = 100;
	if( a > b) {
		if(a > b)System.out.println(a); 
		else System.out.println(c);
			
	} else {
		if(b > c)System.out.println(b);
		else System.out.println(c);
	}
	}
}
